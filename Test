                        if v2.Name == "Top" then
                            v2.Texture = blocktab[v.Name][1]
                            v2.Color3 = v.Name == "grass" and Color3.fromRGB(115, 255, 28) or Color3.fromRGB(255, 255, 255)
                        elseif v2.Name == "Bottom" then
                            v2.Texture = blocktab[v.Name][2]
                        else
                            v2.Texture = blocktab[v.Name][3]
                        end
                    end
                end
